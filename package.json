{
  "name": "@hatkom/prisma-generator-flavoured-ids",
  "author": "Vladyslav Zavalykhatko",
  "description": "Provide a description about your generator",
  "version": "1.0.0",
  "license": "MIT",
  "bin": {
    "prisma-generator-flavoured-ids": "./dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "pnpm build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "@prisma/generator-helper": "^5.4.2",
    "@prisma/internals": "^5.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/prettier": "^2.7.3",
    "prisma": "^5.4.2",
    "typescript": "^5.2.2"
  },
  "homepage": "Link to homepage or github readme here",
  "repository": {
    "type": "git",
    "url": "Repo link here"
  },
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}